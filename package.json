{
  "name": "vue-gmaps",
  "description": "A Google Maps vue plugin",
  "version": "0.0.0-semantic-release",
  "main": "dist/index.js",
  "scripts": {
    "commit": "./node_modules/.bin/git-cz",
    "test": "true",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "keywords": [
    "vue",
    "google-maps"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Ridermansb/vue-gmaps.git"
  },
  "bugs": {
    "url": "https://github.com/Ridermansb/vue-gmaps/issues"
  },
  "author": "ridermansb <ridermansb@gmail.com> (http://ridermansb.me)",
  "license": "ISC",
  "dependencies": {
    "load-google-maps-api": "0.0.2"
  },
  "devDependencies": {
    "cz-conventional-changelog": "1.2.0",
    "semantic-release": "^4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "verifyConditions": "condition-circle"
  }
}
